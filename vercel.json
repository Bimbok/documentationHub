{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        },

        {
            "src": "/logo.ico",
            "dest": "/public/logo.ico"
        }
    ]
}